<!doctype html>
<head>
  <title>{{ page_title }}</title>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="description" content="{{ page_description | escape }}">
  <link rel="canonical" href="{{ canonical_url }}">
  <meta name="viewport" content="width=device-width,initial-scale=1,shrink-to-fit=no">
  {{ content_for_header }} <!-- Header hook for plugins -->
  {{ 'custom.scss.css' | asset_url | stylesheet_tag }}
  {{ 'home-page.scss.css' | asset_url | stylesheet_tag }}
  {{ 'shop.scss.css' | asset_url | stylesheet_tag }}
  {{ 'cart.scss.css' | asset_url | stylesheet_tag }}
  {{ 'help.scss.css' | asset_url | stylesheet_tag }}
  {{ 'learn.scss.css' | asset_url | stylesheet_tag }}
  {{ 'footer.scss.css' | asset_url | stylesheet_tag }}

</head>
<body>
  <div onclick="closeCart()" class="slideout-cart-cover"></div>
  <main role="main">
    {% include 'slideout-cart' %}
    {{ content_for_layout }}
  </main>
</body>
</html>

<script src="https://cdnjs.cloudflare.com/ajax/libs/vue/2.6.10/vue.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/axios/0.19.0/axios.min.js"></script>
<script src="{{ 'slideout-cart.js' | asset_url }}" defer="defer"></script>
